<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head'); %>
</head>
<body>
    <!-- header -->
    <%- include('partials/header'); %>
    <div class="content">
        <div class="teachers-list">
            <% for (var i = 0; i < teachers.length; i++) { %>
                <div class="teacher">
                    <a href="/teachers/<%- teachers[i]._id %>">
                        <%- teachers[i].username %>
                    </a>
                    <img src="<%- teachers[i].avatar %>" alt="Avatar">
                </div>
            <% } %>
        </div>
    </div>
</body>
</html>