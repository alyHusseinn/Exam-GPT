{
  "plugins": ["prettier-plugin-ejs", "prettier-plugin-tailwindcss"],
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "overrides": [
    {
      "files": "*.ejs",
      "options": {
        "parser": "ejs"
      }
    }
  ]
}
